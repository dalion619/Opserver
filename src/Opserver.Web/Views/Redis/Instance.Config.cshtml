@model RedisInstance
@{
    var config = Model.Config.Data;
}
<h4 class="modal-title">
    Server Config
</h4>
@if (config?.Any() ?? false)
{
    <div class="table-responsive">
        <table class="table table-striped table-super-condensed table-hover">
            <thead>
            <tr>
                <th>Config Param</th>
                <th>Value</th>
            </tr>
            </thead>
            <tbody>
            @foreach (var entry in config.OrderBy(e => e.Key))
            {
                <tr>
                    <td>@entry.Key</td>
                    <td>@entry.Value</td>
                </tr>
            }
            </tbody>
        </table>
    </div>
}
else
{
    <div class="no-content">No configuration available</div>
}